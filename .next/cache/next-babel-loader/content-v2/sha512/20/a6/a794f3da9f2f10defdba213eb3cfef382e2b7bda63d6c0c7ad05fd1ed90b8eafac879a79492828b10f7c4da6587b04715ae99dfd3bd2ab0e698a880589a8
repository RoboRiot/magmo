{"ast":null,"code":"// pages/api/slack/inspect-list.js\nexport default async function handler(req, res) {\n  const token = process.env.SLACK_BOT_TOKEN;\n  const {\n    listId\n  } = req.query;\n  if (!token || !listId) return res.status(400).json({\n    error: \"token or listId missing\"\n  });\n  const out = await fetch(\"https://slack.com/api/slackLists.items.list\", {\n    method: \"POST\",\n    headers: {\n      Authorization: `Bearer ${token}`\n    },\n    body: new URLSearchParams({\n      list_id: listId,\n      limit: \"5\"\n    })\n  }).then(r => r.json());\n  res.status(200).json(out);\n}","map":null,"metadata":{},"sourceType":"module"}